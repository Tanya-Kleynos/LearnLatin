@model LearnLatin.Models.InputAnswer

@{
    ViewData["Title"] = "Детали ответа";
}

<h1>Детали ответа</h1>

<div>
    <h6>Тип таска: ввод правильного ответа</h6>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Ответ
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.AnsValue)
        </dd>
        <dt class="col-sm-2">
            Создано
        </dt>
        <dd class="col-sm-10">
            @*@Model.Creator.UserName (@Model.Created)*@
            @Html.DisplayFor(model => model.Created)
        </dd>
        <dt class="col-sm-2">
            Редактировано
        </dt>
        <dd class="col-sm-10">
            @*@Model.Editor.UserName (@Model.Modified)*@
            @Html.DisplayFor(model => model.Modified)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.Id">Редактировать</a> |
    <a asp-action="Index">Назад</a>
</div>
